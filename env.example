# Darwin プロジェクト環境設定例
# このファイルをコピーして .env として使用してください

# Google Cloud Platform
GCP_PROJECT_ID=your-project-id
GCP_REGION=asia-northeast1
GCP_SERVICE_ACCOUNT_KEY=path/to/service-account-key.json

# OpenAI API設定
OPENAI_API_KEY=your_openai_api_key_here
CHATGPT_MODEL=gpt-4
CHATGPT_TEMPERATURE=0.3
CHATGPT_MAX_TOKENS=4000

# My GPTs
MYGPT_API_KEY=your-mygpt-api-key
MYGPT_MODEL=gpt-4o
MYGPT_MAX_TOKENS=4000

# Whisper設定
WHISPER_MODEL=large-v3
WHISPER_DEVICE=auto
WHISPER_COMPUTE_TYPE=auto

# データベース設定
DATABASE_URL=postgresql://user:password@localhost:5432/darwin
RAG_DATABASE_URL=postgresql://user:password@localhost:5432/darwin_rag

# ストレージ設定
GCS_BUCKET_NAME=darwin-storage
GCS_PROJECT_ID=your-gcp-project-id

# Cloud Tasks
TASKS_QUEUE_NAME=darwin-queue
TASKS_BASE_URL=https://darwin-api-xxxxx-uc.a.run.app
TASKS_SERVICE_ACCOUNT_EMAIL=darwin-tasks@your-project.iam.gserviceaccount.com

# Pub/Sub
PUBSUB_TOPIC_NAME=darwin-topic
PUBSUB_SUBSCRIPTION_NAME=darwin-subscription

# Cloud Logging
LOG_NAME=darwin-app
LOG_LEVEL=INFO
LOG_FILE=app.log

# CloudFlare
CLOUDFLARE_API_TOKEN='7cA9KkJOz5T86j95kbAaMtKcQ4H2J2FlifDGiR9z'
CLOUDFLARE_ZONE_ID=your-zone-id
CLOUDFLARE_DOMAIN=sambo-office.com
CLOUDFLARE_SUBDOMAIN=darwin

# Vercel
VERCEL_ORG_ID=your-vercel-org-id
VERCEL_PROJECT_ID=your-vercel-project-id
VERCEL_TOKEN=your-vercel-token

# GitHub Actions
GCP_SA_KEY=your-service-account-key-json

# アプリケーション設定
PROJECT_NAME=Darwin
DEBUG=false
